{
  "name": "mazelike",
  "version": "0.0.0",
  "scripts": {
    "test": "jasmine --config=tests/spec/support/jasmine.json",
    "test-browser": "node tests/browser-ci",
    "start": "sequelize db:migrate && node --experimental-modules Backend/index.mjs",
    "doc": "jsdoc -r Frontend/game Backend doc.md -c jsdoc.json -d Documents",
    "eslint": "eslint Frontend Backend --ext .js,.mjs",
    "debug": "node --experimental-modules --inspect Backend/index.mjs",
    "db:migrate": "sequelize db:migrate",
    "db:migrate:undo": "sequelize db:migrate:undo",
    "migration:create": "sequelize migration:create --name"
  },
  "husky": {
    "hooks": {
      "pre-push": "jsdoc -r Frontend/game Backend doc.md -c jsdoc.json -d docs && git add docs/* && git commit -m 'Doc: JSDoc'"
    }
  },
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "chalk": "^2.4.1",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-handlebars": "^3.0.0",
    "express-session": "^1.15.6",
    "moment": "^2.22.2",
    "morgan": "^1.9.1",
    "multer": "^1.4.0",
    "mysql2": "^1.6.1",
    "nodemailer": "^4.6.8",
    "pg": "^7.7.1",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.38.1",
    "sequelize-cli": "^4.1.1",
    "socket.io": "^2.1.1",
    "spdy": "^4.0.0",
    "sqlite3": "^4.0.4",
    "winston": "^3.1.0"
  },
  "private": true,
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.2",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.1.2",
    "babel-preset-minify": "^0.5.0",
    "chai": "^4.1.2",
    "eslint": "^5.5.0",
    "husky": "^1.2.0",
    "jasmine": "^3.2.0",
    "jasmine-core": "^3.2.1",
    "jsdoc": "^3.5.5",
    "puppeteer": "1.5.0",
    "request": "^2.88.0",
    "rollup": "^0.67.3"
  }
}
